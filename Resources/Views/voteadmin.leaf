<!doctype html>
<html lang="en">
<head>
<meta charset="utf-8">
<link rel="stylesheet" type="text/css" href="/css/css.css">
<script src="/js/js.js"></script>
<title>#(title)</title>

</head>

<body>
<h1>Your '<i>#(title)</i>' vote</h1>
<hr>
Vote link: <a href="#(voteLink)">vote.smkid.dk#(voteLink)</a>
<br><br>
<a href="" class="blue button">Reload page</a><br><hr>
<br>
<table>
	<tr>
		#for(head in headers):
			<th>#(head)</th>
		#endfor
		</tr>
		#for(user in userStatus):
			<tr>
				<td>#(user.user.identifier)</td>
				<td>#(user.user.name)</td>

				<td align="center">
				#if(user.hasVoted):
					<span class="dot green"></span>
				#else:
					<span class="dot red"></span>
				#endif
				</td>
				<td align="center">
				#if(user.isEligibleToVote):
					<span class="dot green"></span>
				#else:
					<span class="dot red"></span>
				#endif
				</td>
				
				<td align="center">
				#if(user.hasVoted):
				<form method='post' action="/reset/#(user.user.id)" onsubmit="lockButton()">
				<input type="submit" class="inlinebutton red" value="Reset vote">
				</form>
				#endif
				</td>
			</tr>
		#endfor
		</table>
<hr>

#if(showGetResults):
<form method='post'>
<input type="hidden" name="statusChange" value="getResults">
<input type="submit" class="button green" value="Get results">
</form>
#endif

#if(showClose):
<form method='post'>
<input type="hidden" name="statusChange" value="close">
<input type="submit" class="button red" value="Close vote">
</form>
#endif

#if(showOpen):
<form method='post'>
<input type="hidden" name="statusChange" value="open">
<input type="submit" class="button green" value="Open vote">
</form>
#endif

<hr>
</body>
</html>
